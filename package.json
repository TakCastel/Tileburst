{
  "name": "tileburst",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build && cp screenshot.png dist/screenshot.png && cp src/manifest.json dist/manifest.json && cp src/service-worker.js dist/service-worker.js",
    "preview": "ng serve --configuration=production",
    "cap:init": "npx cap init",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:copy": "npx cap copy",
    "cap:run:ios": "npx cap run ios",
    "cap:run:android": "npx cap run android"
  },
  "dependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "html2canvas": "^1.4.1",
    "lucide-angular": "^0.562.0",
    "rxjs": "^7.8.2",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "typescript": "~5.9.0",
    "vite": "^6.2.0"
  }
}
